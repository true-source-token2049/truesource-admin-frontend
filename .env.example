# Alchemy API Key for Ethereum Sepolia (get from https://alchemy.com)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here

# Contract address of your deployed EditableNFT contract
NEXT_PUBLIC_CONTRACT_ADDRESS=your_contract_address_here

# Private key for deploying contracts and minting (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Pinata API credentials (get from https://pinata.cloud)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here
PINATA_ACCESS_TOKEN=your_pinata_access_token_here
